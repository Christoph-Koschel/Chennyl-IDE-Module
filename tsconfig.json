  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "strict": true,
    "target": "es5",
    "module": "CommonJS",
    "lib": [
      "ES2015",
      "DOM"
    ],
    "types": [
      "node"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
